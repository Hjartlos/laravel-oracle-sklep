create TYPE TRANSACTION_STATS_OBJ AS OBJECT (
    LICZBA_TRANSAKCJI        NUMBER,
    SUMA_TRANSAKCJI          NUMBER,
    SREDNIA_WARTOSC          NUMBER,
    NAJMNIEJSZA_TRANSAKCJA   NUMBER,
    NAJWIEKSZA_TRANSAKCJA    NUMBER
)
/

create TYPE CITY_STATS_OBJ AS OBJECT (
    MIEJSCOWOSC               VARCHAR2(100),
    LICZBA_KLIENTOW           NUMBER,
    LICZBA_TRANSAKCJI         NUMBER,
    LACZNA_WARTOSC_ZAKUPOW    NUMBER
)
/

create TYPE WORKER_STATS_OBJ AS OBJECT (
    PRACOWNIKID       NUMBER,
    IMIE              VARCHAR2(50),
    NAZWISKO          VARCHAR2(50),
    LICZBA_TRANSAKCJI NUMBER,
    WARTOSC_SPRZEDAZY NUMBER
)
/

create TYPE CLIENT_STATS_OBJ AS OBJECT (
    KLIENTID          NUMBER,
    IMIE              VARCHAR2(50),
    NAZWISKO          VARCHAR2(50),
    LICZBA_TRANSAKCJI NUMBER,
    SUMA_WYDATKOW     NUMBER
)
/

create TYPE PRODUCT_STATS_OBJ AS OBJECT (
    NAZWA_BRONI              VARCHAR2(100),
    PRZYKLADOWE_PRODUKTID    NUMBER,
    KATEGORIA                VARCHAR2(100),
    LICZBA_ZAMOWIEN          NUMBER,
    LACZNA_ILOSC             NUMBER,
    LACZNA_WARTOSC_SPRZEDAZY NUMBER
)
/


